<!DOCTYPE html>
<html>
<head>
    <meta charset="utf-8" />
    <meta name="viewport" content="width=device-width, initial-scale=1.0">
    <title>@ViewBag.Title - PAYC DEMO ARTICULACIÓN CONTABILIDAD</title>
    @*Del theme de Polifonía*@
    @*<link href="~/Content/bootstrap.csfs" rel="stylesheet" />*@

    <link href="~/Content/ej/ej.widgets.core.min.css" rel="stylesheet" />
    <link href="~/Content/ej/bootstrap-theme/ej.theme.min.css" rel="stylesheet" />


    @*theme polifonía*@


    @Styles.Render("~/Content/css")


    <link href="~/Content/Site2.css" rel="stylesheet" />
    <link href="~/Content/main2.css" rel="stylesheet" />
    <link href="~/Content/main.css" rel="stylesheet" />

    @*<link rel="stylesheet" href="//code.jquery.com/ui/1.12.1/themes/base/jquery-ui.css">
    <link rel="stylesheet" href="/resources/demos/style.css">
    <script src="https://code.jquery.com/jquery-1.12.4.js"></script>
    <script src="https://code.jquery.com/ui/1.12.1/jquery-ui.js"></script>*@


    <link href="~/Content/bootstrap.css" rel="stylesheet" />

    <link href="~/Content/ej/ej.widgets.core.min.css" rel="stylesheet" />
    <link href="~/Content/ej/bootstrap-theme/ej.theme.min.css" rel="stylesheet" />
    <link href="~/Content/ej/bootstrap-theme/ej.web.all.min.css" rel="stylesheet" />
    <script src="~/Scripts/jquery-1.11.3.min.js" type="text/javascript"></script>
    <script src="~/Scripts/jquery-ui-min.js" type="text/javascript"></script>
    <script src="~/Scripts/jquery.validate.min.js"></script>
    <script src="~/Scripts/jquery.validate.unobtrusive.min.js"></script>
    <script src="~/Scripts/jquery.easing-1.3.min.js" type="text/javascript"></script>
    <script src="~/Scripts/ej/ej.web.all.min.js" type="text/javascript"></script>
    <!--<script src="~/Scripts/ej/ej.webform.min.js" type="text/javascript"></script>-->
    <script src="~/Scripts/ej/ej.webform.js" type="text/javascript"></script>
    <!--<script src="~/Scripts/webformsmin.js"></script>-->
    <script src="~/Scripts/bootstrap.min.js" type="text/javascript"></script>
    <script src="~/Scripts/jquery.wizard.js"></script>
    <script src="~/Scripts/jsrender.min.js"></script>
    <script src="~/Scripts/traduccion/ej.culture.es-CO.min.js"></script>
    <script src="~/Scripts/traduccion/ej.localetexts.es-CO.min.js"></script>

    <script src="~/Scripts/require.min.js"></script>
</head>
<body>
    <!-- /Loader-->
    <div class="contenedor_loader">
        <span class="e-image imagen_loader"></span>
    </div>
    <div class="contenedor_loader1">
        <img class="e-image imagen_loader" src="~/images/waitingpopup.gif">

    </div>
    <!-- /Contenido-General-->
    <div class="contenedor_universal">
        <div id="wrapper">
            <!-- Menu Top -->

            <nav class="menu-top navbar navbar-default navbar-fixed-top">

                <div class="container-fluid" style="padding: 0px 70px;">
                    <div class="logo"><img class="logopayc" src="~/images/logopayc.png" /></div>
                    <div class="navbar-header">
                        <button aria-controls="navbar" aria-expanded="false" data-target="#navbar" data-toggle="collapse" class="navbar-toggle collapsed" type="button">
                            <span class="sr-only">Toggle navigation</span>
                            <span class="icon-bar"></span>
                            <span class="icon-bar"></span>
                            <span class="icon-bar"></span>
                        </button>
                        @Html.Partial("_LoginPartial")
                        <a href="" class="navbar-brand">INTEGRAPP PAYC</a>
                    </div>
                    <div class="navbar-collapse collapse" id="navbar">
                        <ul class="nav navbar-nav navbar-right"></ul>
                    </div><!--/.nav-collapse -->
                </div>
            </nav>
            <!-- /Menu Top -->
            <!-- Sidebar-wrapper  -->
            <nav class="navbar navbar-inverse navbar-fixed-top" id="sidebar-wrapper" role="navigation">
                <ul class="nav sidebar-nav">
                    <h5 style="font-size:30px;">+</h5>
                    @*Selección de tipo de gestión*@
                    <div class="seleccion_tipo_gestion">
                        <div class="col-md-12">
                            <h4>SELECCIONAR TIPO GESTIÓN</h4>
                            <select id="drop_tipo_gestion">
                                <option value="item1">INTERNA</option>
                                <option value="item2">EXTERNA</option>
                            </select>
                        </div>
                    </div>
                    @*Selección de acción a realizar*@
                    <div class="botones">
                        <div class="col-md-12">
                            <h4>SELECCIONAR ACCIÓN A REALIZAR</h4>
                            <div class="col-md-1" style="padding-right:50px;">
                                <a class="btn btn-purple email" onclick="enviar_gestion($(this))"></a>
                            </div>
                            <div class="col-md-1" style="padding-right:50px;">
                                <a class="btn btn-purple whatsapp" onclick="enviar_gestion($(this))"></a>
                            </div>
                            <div class="col-md-1" style="padding-right:50px;">
                                <a class="btn btn-purple skype" onclick="enviar_gestion($(this))"></a>
                            </div>
                        </div>
                    </div>
                    @*Envío de acción*@
                    <div class="contenedor_cartera">

                        <div class="col-md-12 div_email">
                            <h4>ENVIAR EMAIL</h4>
                            <input type="text" class="form-control input_cliente" id="correo_cliente1" placeholder="CORREO ELECTRÓNICO">
                            <input type="text" class="form-control input_responsable" id="correo_responsable1" placeholder="CORREO ELECTRÓNICO">

                            <div class="contenedor_textos input_cliente">
                                <textarea id="texto_correo" rows="10" style="width: 100%; display:none !important">
                                </textarea>
                            </div>
                            <div class="contenedor_textos input_responsable">
                                <textarea id="texto_correo_responsable" rows="10" style="width: 100%; display:none !important">
                                </textarea>
                            </div>
                            <a class="enviar_email_gesion btn btn-purple accion_gestion" onclick="gestion_cartera($(this))">ENVIAR</a>
                            <a class="cancelar btn btn-purple accion_gestion" onclick="cancelar()">CANCELAR</a>
                        </div>
                        <div class="col-md-12 div_whatsapp">
                            <h4>ENVIAR MENSAJE</h4>
                            <input type="text" class="form-control input_cliente" id="telefono_whatsapp_cliente" placeholder="TELÉFONO WHATSAPP">
                            <input type="text" class="form-control input_responsable" id="telefono_whatsapp_responsable" placeholder="TELÉFONO WHATSAPP">
                            <h4>DESCRIPCIÓN MENSAJE</h4>
                            <textarea id="descripcion_whatsapp" rows="5" style="width: 100%; margin-top:30px"></textarea>
                            <a class="enviar_whatsapp_gesion btn btn-purple accion_gestion" onclick="gestion_cartera($(this))">REGISTRAR</a>
                            <a class="cancelar btn btn-purple accion_gestion" onclick="cancelar()">CANCELAR</a>
                        </div>
                        <div class="col-md-12 div_skype">
                            <h4>LLAMAR</h4>
                            <input type="text" class="form-control input_cliente" id="telefono_llamar_cliente" placeholder="TELÉFONO">
                            <input type="text" class="form-control input_responsable" id="telefono_llamar_responsable" placeholder="TELÉFONO">
                            <h4>DESCRIPCIÓN LLAMADA</h4>
                            <textarea id="descripcion_llamar" rows="5" style="width: 100%; margin-top:30px"></textarea>
                            <a class="enviar_skype_gesion btn btn-purple accion_gestion" onclick="gestion_cartera($(this))">REGISTRAR</a>
                            <a class="cancelar btn btn-purple accion_gestion" onclick="cancelar()">CANCELAR</a>
                        </div>
                    </div>
                </ul>
            </nav>

            <!-- /#sidebar-wrapper -->
            <!-- Contenido-General -->
            <div id="page-content-wrapper">
                <div class="container-fluid">
                    <div class="row">

                        <!-- Columna Menu Left -->
                        <div class="col-xs-1 col-lg-1">

                            <div class="menu-left">
                                <button class="alert-open is-closed animated fadeInLeft btn-lg" type="button" data-toggle="offcanvas">
                                    <span class="glyphicon glyphicon-bullhorn beige"></span>
                                </button>
                                @if (Request.IsAuthenticated)
                                {
                                    <ul>
                                        <!--COMERCIAL-->
                                        <li class="left-icons icon-entida dropdown-submenu" title="GESTIÓN COMERCIAL">
                                            <ul class="dropdown-menu derecha">
                                                @*<h6>@Html.ActionLink("FLUJO PROYECTOS", "Index", "PROYECTOS")</h6>*@
                                                <h6>@Html.ActionLink("ORDENES DE SERVICIO", "Index", "FlujoProyectos/Index")</h6>
                                                <h6>@Html.ActionLink("PROYECTOS", "Index", "PROYECTOS/Index")</h6>
                                                <h6>@Html.ActionLink("CLIENTES", "Index", "CLIENTES/Index")</h6>
                                                <h6>@Html.ActionLink("CONTRATOS", "Index", "CONTRATOS/Index")</h6>
                                                <h6>@Html.ActionLink("CONTRATOS CONDICIONES", "Index", "CONTRATOS_CONDICIONES/Index")</h6>
                                                <h6>@Html.ActionLink("CONTRATOS ROL", "Index", "CONTRATOS_ROL/Index")</h6>
                                                <h6>@Html.ActionLink("ITEMS CONTRATOS", "Index", "ITEMS_CONTRATO/Index")</h6>
                                                <h6>@Html.ActionLink("FLUJO INGRESOS RRHH", "Index", "FLUJO_INGRESOS_ROL/Index")</h6>
                                                <h6>@Html.ActionLink("FLUJO INGRESOS ITEMS OTROS COSTOS", "Index", "FLUJO_INGRESOS_ITEMS/Index")</h6>
                                                <h6>@Html.ActionLink("TIPOS CONDICIONES", "Index", "TIPO_CONDICION_CONTRACTUAL/Index")</h6>

                                            </ul>
                                        </li>
                                        <!--GESTION HUMANA-->
                                        <li class="left-icons icon-personaj dropdown-submenu" title="GESTIÓN HUMANA">
                                            <ul class="dropdown-menu derecha">
                                                <h6>@Html.ActionLink("ASIGNACIONES PENDIENTES", "Index", "CONTRATO_COLABORADOR/Pendientes")</h6>
                                                <h6>@Html.ActionLink("ASIGNACIONES A PROYECTOS", "Index", "CONTRATO_COLABORADOR/Index")</h6>
                                                <h6>@Html.ActionLink("REGISTRO NOVEDADES", "Index", "REGISTRO_NOVEDADES/Index")</h6>
                                                <h6>@Html.ActionLink("COLABORADORES", "Index", "COLABORADORES/Index")</h6>
                                                <h6>@Html.ActionLink("ALERTA RETIROS", "Retiros", "COLABORADORES")</h6>

                                                <h6>@Html.ActionLink("CARGOS", "Index", "CARGOS/Index")</h6>
                                                <h6>@Html.ActionLink("ROLES", "Index", "ROLES/Index")</h6>
                                                <h6>@Html.ActionLink("TIPOS NOVEDADES", "Index", "TIPOS_NOVEDAD/Index")</h6>
                                            </ul>
                                        </li>
                                        <!--FINANCIERA-->
                                        <li class="left-icons icon-parrill dropdown-submenu" title="GESTIÓN FINANCIERA">
                                            <ul class="dropdown-menu derecha">
                                                <h6>@Html.ActionLink("FACTURACIÓN", "index", "FACTURAS/index")</h6>

                                                <h6>@Html.ActionLink("REGISTRO ITEMS REEMBOLSABLES", "index", "REGISTRO_ITEMS_OTROS_COSTOS/index")</h6>
                                                <h6>@Html.ActionLink("ITEMS PRESUPUESTALES", "index", "ITEMS/index")</h6>
                                                <h6>@Html.ActionLink("CENTROS DE COSTO", "index", "VISTA_ORDENES_CENTRO_COSTOS/index")</h6>
                                                <h6>@Html.ActionLink("CARTERA", "index", "VISTA_CARTERA_INDEX/index")</h6>
                                            </ul>
                                        </li>
                                        <!--ADMINSTRACIÓN USUARIOS-->
                                        <li class="left-icons icon-general dropdown-submenu">

                                            <ul class="dropdown-menu derecha">
                                                <h6>@Html.ActionLink("REGISTRAR USUARIO", "Register", "Account")</h6>
                                            </ul>
                                        </li>
                                    </ul>
                                }
                            </div>
                        </div>
                        <!-- /Columna Menu Left -->
                        <!-- Columna Contenido -->
                        <div class="col-xs-11 col-lg-11 contenedor_tablas">

                            @RenderBody()
                            <hr />


                        </div>
                    </div>
                    <!-- /Columna Contenido -->
                </div>
            </div>
        </div>
    </div>
    </div>
    <!-- /Footer-->
    <footer>
        <div class="container body-content">
            <hr />
            <p>&copy; @DateTime.Now.Year - PAYC SAS</p>
        </div>

    </footer>



    @RenderSection("scripts", required: false)


    @(Html.EJ().ScriptManager())
    
    <script type="text/javascript">

    </script>
    <script src="~/Scripts/main.js" type="text/javascript"></script>
</body>
</html>

